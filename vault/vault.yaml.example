---
# Vault secrets

# SSH secrets
secret_ssh_keys:
  - name: "Device Name 1"
    key: change_me
  - name: "Device Name 2"
    key: change_me

# Digitalocean secrets
secret_do_token: change_me

# Cloudflare secrets
secret_cf_token: change_me

# Default user values
secret_user_password: change_me
secret_user_password_prehashed: hashed version of 'secret_user_password'
secret_become_pass: change_me
secret_become_pass_arnie: change_me

# Healthchecks.io secrets
secret_hc_ping_key: change_me
secret_hc_snapraid_uuid: change_me

# Postfix secrets
secret_postfix_mg_storage_password: get from mailgun

# Unifi OS, Controller, and Protect secrets
secret_unifi_username_password: change_me

# Ntfy secrets
secret_ntfy_password: change_me
secret_ntfy_mailgun_smtp_password: get from mailgun

# Traefik secrets
secret_traefik_dashboard_password: change_me
# Hash generated from https://hostingcanada.org/htpasswd-generator/ with mode bcrypt
secret_traefik_dashboard_password_hash: hashed version of 'secret_traefik_dashboard_password'

# Healthchecks secrets
secret_hc_mg_password: get from mailgun
secret_hc_secret_key: change_me
secret_hc_db_password: change_me
secret_hc_superuser_password: change_me

secret_hc_projects:
  "Project Name 1":
    api_key: 24 byte key
    ping_key: 16 byte key
  "Project Name 2":
    api_key: 24 byte key
    ping_key: 16 byte key

# Tailscale secrets
secret_ts_client_id: change_me
secret_ts_client_secret: change_me

# Pihole secrets
secret_pihole_password: change_me
secret_pihole_password_hash: Get_from_pihole_toml_config_file_if_not_already_set_in_vault
 Pihole application password
secret_pihole_app_password: Get_from_web_interface_if_not_already_set_in_vault
# Pihole application password hash
secret_pihole_app_password_hash: Get_from_pihole_toml_config_file_if_not_already_set_in_vault


# Proxmox or PVE secrets
secret_pve_prometheus_user_password: change_me
secret_pve_homepage_user_password: change_me
# token available through the Proxmox GUI
secret_pve_homepage_token: change_me

# PBS secrets
secret_pbs_homepage_user_password: change_me
# token available through the Proxmox PBS GUI
secret_pbs_homepage_token: change_me

# Grafana secrets
secret_grafana_password: change_me

# WUD Docker Hub secrets
secret_docker_hub_wud_access_token: get_from_docker_hub

# Nut secrets
secret_nut_admin_password: change_me
secret_nut_mon_password: change_me

# ZFS datasets encryption secrets
secret_zfs_datasets_encryption:
  "Dataset Name 1":
    passphrase: change_me
  "Dataset Name 2":
    passphrase: change_me

# Rclone secrets
secret_rclone_user_aws_access_key: change_me
secret_rclone_user_aws_secret_key: change_me

secret_rclone_password:
  - password: rclone obscure "$(openssl rand -base64 32)"
    salt: rclone obscure "$(openssl rand -base64 32)"
  - password: rclone obscure "$(openssl rand -base64 32)"
    salt: rclone obscure "$(openssl rand -base64 32)"

# Wifi secrets
secret_default_wifi_password: change_me
